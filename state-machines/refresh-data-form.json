{
  "Comment": "Refresh Ofsted Data via CSV File",
  "name": "Refresh Ofsted Data via CSV File",
  "version": "1.0",
  "categories": [
    "dataManagement"
  ],
  "instigators": [
    "user"
  ],
  "StartAt": "FillForm",
  "States": {
    "FillForm": {
      "Type": "Task",
      "Resource": "module:awaitingHumanInput",
      "ResourceConfig": {
        "uiType": "form",
        "uiName": "ofsted_refreshDataForm",
        "dataPath": "$"
      },
      "Next": "Insert"
    },
    "Insert": {
      "Type": "Task",
      "Resource": "function:ofsted_refreshDataInsert",
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "dm_dataManager",
      "allows": [
        "*"
      ]
    }
  ]
}
