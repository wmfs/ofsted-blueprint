{
  "title": "ofsted",
  "description": "Gets Ofsted inspection data from Ofsted CSV file and imports into Postgres for use in RIDGE models",
  "primaryKey": [
    "urn"
  ],
  "type": "object",
  "properties": {
    "urn": {
      "type": "integer",
      "maxLength": 12
    },
    "schoolName": {
      "type": "string"
    },
    "ofstedPhase": {
      "type": "string"
    },
    "region": {
      "type": "string"
    },
    "localAuthority": {
      "type": "string"
    },
    "postcode": {
      "type": "string"
    },
    "inspectionDate": {
      "type": "string",
      "format": "date-time"
    },
    "overallEffectiveness": {
      "type": "integer"
    },
    "effectivenessOfLeadership": {
      "type": "integer"
    }
  }
}

